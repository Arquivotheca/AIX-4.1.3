# Stanzas for populating the 4MEG LTBL Device Driver predefined values
# See src/bos/usr/lib/methods/common/devices.msg for nls messages
#

PdDv:
	type = "14103c00"
	class = "adapter"
	subclass = "pci"
	prefix = "ltbl"		# ltbl ?
	devid = "0x14103c00"	# need device id
	base = 0
	has_vpd = 1
 	detectable = 1
	chgstatus = 0
	bus_ext = 0
	fru = 1			# daughter card ?
	led = 0x999		# need LED value for AWS machines
	setno = 45
	msgno =  00		# need msgno
	catalog = "devices.cat"
	DvDr = "ltbldd"
	Define = "/usr/lib/methods/define_rspc"
	Configure = "/usr/lib/methods/cfg_ltbl"
	Change = "/usr/lib/methods/chggen"
	Unconfigure = "/usr/lib/methods/ucfggraphics"
	Undefine = "/usr/lib/methods/undefine"
        Start = ""
        Stop = ""
	uniquetype = "adapter/pci/14103c00"

