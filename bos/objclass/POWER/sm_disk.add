#
# Add a Disk
# This allows a disk device to be added by defining and configuring it.
# There are two name selectors followed by the dialogue.  The first
# name selector is used to put up a list of Predefined disk devices for the
# user to select from.  The second name selector depends on the type of
# disk selected.  If the disk is a scsi disk (including osdisk), the
# name selector puts up a list of defined SCSI adapters which can have
# the selected disk attached to it.  The SCSI dialogue then puts up a 
# list of all of the user configurable attributes.  Direct attached
# and serial disks cannot be added.

# Select type of disk
sm_name_hdr:
    id                        = "makdsk"
    next_id                   = "makdsk_"
    option_id                 = "disk_predef_opt"
    has_name_select           = "n"
    name                      = "Add a Disk"
    name_msg_file             = "smit.cat"
    name_msg_set              = 25
    name_msg_id               = 97
    type                      = "c"
    ghost                     = "y"
    cmd_to_classify           = "\
class () # str1 str2 str3\n\
{\n\
echo $1 | sed 's/^\\([^ ]*\\)[ ]*\\([^ ]*\\)[ ]*/\\2:\\1:\\2:/' \
     |sed s/"scsi:osdisk"/"osdisk_scsi:osdisk"/g \n\
}\n\
class "
    cmd_to_classify_postfix   = "_rawname"
    raw_field_name            = ""
    cooked_field_name         = "_cookedname:type:subclass:desc"
    next_type                 = "n"
    help_msg_id               = "1800102"
    help_msg_loc              = ""

# Name selector command option for type of disk
sm_cmd_opt:
    id                        = "disk_predef_opt"
    id_seq_num                = "0"
    disc_field_name           = ""
    name                      = "Disk Type"
    name_msg_file             = "smit.cat"
    name_msg_set              = 27
    name_msg_id               = 401
    op_type                   = "l"
    entry_type                = "t"
    entry_size                = 0
    required                  = "y"
    prefix                    = ""
    cmd_to_list_mode          = "a"
    cmd_to_list               = "lsdev -P -c disk -F 'type subclass description' | awk '!/mca/ && !/serdasdc/' -"
    cmd_to_list_postfix       = ""
    multi_select              = ""
    value_index               = 0
    disp_values               = ""
    values_msg_file           = ""
    values_msg_set            = 0
    values_msg_id             = 0
    aix_values                = ""
    help_msg_id               = "04201"
    help_msg_loc              = ""

