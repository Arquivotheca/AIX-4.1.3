#
# Change/Show Characteristics of a Disk
# This allows a disk device's characteristics to be shown and, if desired,
# changed.  First, there is a name selector used to put up a list of the
# defined disks for the user to select from.  The dialogue then
# shows all of the characteristics.

# Select disk by logical name
sm_name_hdr:
    id                        = "chgdsk"
    next_id                   = "chgdsk_"
    option_id                 = "disk_ln_opt"
    has_name_select           = "n"
    name                      = "Change/Show Characteristics of a Disk"
    name_msg_file             = "smit.cat"
    name_msg_set              = 25
    name_msg_id               = 98
    type                      = "c"
    ghost                     = "y"
    cmd_to_classify           = "\
xx () \n\
{ \n\
lsdev -C -l $1 -F \"type:subclass:parent:connwhere:location:status:description\" \
  | sed 's/^\\([^:]*\\):\\([^:]*\\):/\\2:&/'\\
  |sed s/"ide:oidisk"/"oidisk_ide:oidisk"/g \\
  |sed s/"scsi:osdisk"/"osdisk_scsi:osdisk"/g \n\
} \n\
xx "
    cmd_to_classify_postfix   = " logicname "
    raw_field_name            = "logicname"
    cooked_field_name         = "_cookedname:type:subclass:parent:port:loc:state:desc"
    next_type                 = "d"
    help_msg_id               = "1800197"
    help_msg_loc              = ""


