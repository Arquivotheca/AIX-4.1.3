sm_cmd_opt:
	id_seq_num = "0"
	id = "RCS_rcs_serv_sel"
	disc_field_name = ""
	name = "Select a Remote Service"
	name_msg_file = "rcs.cat"
	name_msg_set = 1
	name_msg_id = 56
	op_type = "l"
	entry_type = ""
	entry_size = 0
	required = "+"
	prefix = ""
	cmd_to_list_mode = "1"
	cmd_to_list = "x() \n\
{ \n\
lsusrprof -l -n $1 > /dev/null 2>&1 \n\
RC2=$? \n\
if [ $RC2 -eq 2 ] \n\
then \n\
	dspmsg ksh.cat 1 'Permission denied.' \n\
	exit 1 \n\
fi \n\
LINK=0 \n\
for I in `lsusrprof -l -n $1 | cut -d':' -f7` \n\
do \n\
	case $I in \n\
		3) LINK=1;; \n\
		4) LINK=1;; \n\
		5) LINK=1;; \n\
	esac \n\
done \n\
if [ $LINK -eq 1 ] \n\
then \n\
	dspmsg rcs.cat 33 'IBMLink\n' \n\
fi \n\
} \n\
x "
