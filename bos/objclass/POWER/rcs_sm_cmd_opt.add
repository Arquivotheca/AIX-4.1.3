	case $I in \n\
		3) echo `lsusrprof -l -n $1 -t 3|cut -d ':' -f2,3|sed 's/:/ /g'`;TELNET=1;;\n\
		4) echo `lsusrprof -l -n $1 -t 4|cut -d ':' -f2,3|sed 's/:/ /g'`;SLIP=1;;\n\
		5) echo `lsusrprof -l -n $1 -t 5|cut -d ':' -f2,3|sed 's/:/ /g'`;SNA=1;;\n\
	esac \n\
done\n\
if [ $TELNET -eq 0 ] && [ SLIP -eq 0 ] && [ SNA -eq 0 ] \n\
then \n\
	dspmsg rcs.cat 71 'You do not have any profiles of this type.\n' \n\
	dspmsg rcs.cat 21 'Contact your system administrator.\n' \n\
exit 1 \n\
