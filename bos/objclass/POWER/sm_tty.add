    id                        = "maktty_hdr.opt"
    id_seq_num                = "0"
    disc_field_name           = ""
    name                      = ""
    op_type                   = ""
    entry_type                = ""
    entry_size                = 0
    required                  = ""
    cmd_to_list_mode          = ""
    cmd_to_list               = ""

# The dialogue headers follow.  There is one for each tty
# based on the driver controlling the parent adapter
# Their id's are built as follows:
#	"maktty_hdr_xxx"
# where xxx is the driver type i.e. rsdd, liondd, cxmadd.

# dialog for tty on an 8-, or 16-port adapter
# or on one of the standard i/o serial ports

sm_cmd_hdr:
    id                        = "maktty_hdr_rsdd"
    option_id                 = "tty_add,tty_common,tty_rsdd"
    has_name_select           = "y"
    name                      = "Add a TTY"
    name_msg_file             = "smit.cat"
    name_msg_set              = 25
    name_msg_id               = 83
    cmd_to_exec               = "mkdev -c tty "
    ask                       = ""
    exec_mode                 = ""
    ghost                     = "n"
    cmd_to_discover = "attribs () \n\
{ \n\
l1=$( lsattr -c tty -s $1 -t $2 -D -O ) \n\
l11=$( echo \"$l1\" | head -1 ) \n\
l12=$( echo \"$l1\" | tail -1 ) \n\
l2=$( lsattr -l $3 -D -O ) \n\
l21=$( echo \"$l2\" | head -1 | sed -e 's/.//' ) \n\
l22=$( echo \"$l2\" | tail -1 ) \n\
echo \"$l11:$l21\" \n\
echo \"$l12:$l22\" \n\
} \n\
attribs "
    cmd_to_discover_postfix = "subclass type parent"
    name_size                 = 0
    value_size                = 0
    help_msg_id               = "180080"
    help_msg_loc              = ""

