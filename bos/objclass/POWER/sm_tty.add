    option_id                 = ""
    has_name_select           = "n"
    name                      = "List All Defined TTYs"
    name_msg_file             = "smit.cat"
    name_msg_set              = 25
    name_msg_id               = 82
    cmd_to_exec               = "lsdev -C -c tty -H"
    ask                       = ""
    exec_mode                 = ""
    ghost                     = "y"
    cmd_to_discover           = ""
    cmd_to_discover_postfix   = ""
    name_size                 = 0
    value_size                = 0
    help_msg_id               = "180079"
    help_msg_loc              = ""

#
# Add a TTY
# This allows a TTY device to be added by defining and configuring it.
# There are two name selectors followed by the dialogue.  The first
# name selector is used to put up a list of Predefined TTY devices for the
# user to select from.  The second name selector puts up a list of
# defined serial adapters which can have the selected type of TTY
# attached to it.  Then there is the  dialogue header for
# for putting up all the user configurable attributes.

# Select type of TTY
sm_name_hdr:
    id                        = "maktty"
    next_id                   = "maktty_parent"
    option_id                 = "tty_predef_opt"
    has_name_select           = "n"
    name                      = "Add a TTY"
    name_msg_file             = "smit.cat"
    name_msg_set              = 25
    name_msg_id               = 83
    type                      = "j"
    ghost                     = "y"
    cmd_to_classify           = "\
class () # str1 str2 str3\n\
{\n\
echo $1 | sed 's/^\\([^ ]*\\)[ ]*\\([^ ]*\\)[ ]*/\\1:\\2:/'\n\
}\n\
class "
    cmd_to_classify_postfix   = "_rawname"
    raw_field_name            = ""
    cooked_field_name         = "type:subclass:desc"
    next_type                 = "n"
    help_msg_id               = "180080"
    help_msg_loc              = ""

# Name selector command option for type of TTY
sm_cmd_opt:
    id                        = "tty_predef_opt"
    id_seq_num                = "0"
    disc_field_name           = ""
    name                      = "TTY Type"
    name_msg_file             = "smit.cat"
    name_msg_set              = 27
    name_msg_id               = 101
    op_type                   = "l"
    entry_type                = "t"
    entry_size                = 0
    required                  = "y"
    prefix                    = ""
    cmd_to_list_mode          = "a"
    cmd_to_list               = "lsdev -P -c tty -F \"type subclass description\""
    cmd_to_list_postfix       = ""
    multi_select              = ""
    value_index               = 0
    disp_values               = ""
    values_msg_file           = ""
    values_msg_set            = 0
    values_msg_id             = 0
    aix_values                = ""
    help_msg_id               = "04201"
    help_msg_loc              = ""

