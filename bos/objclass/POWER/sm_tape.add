    id_seq_num                = "0"
    disc_field_name           = ""
    name                      = "Tape Drive Type"
    name_msg_file             = "smit.cat"
    name_msg_set              = 27
    name_msg_id               = 476
    op_type                   = "l"
    entry_type                = "t"
    entry_size                = 0
    required                  = "y"
    prefix                    = ""
    cmd_to_list_mode          = "a"
    cmd_to_list               = "lsdev -P -c tape -F \"type subclass description \"" | sort
    cmd_to_list_postfix       = ""
    multi_select              = ""
    value_index               = 0
    disp_values               = ""
    values_msg_file           = ""
    values_msg_set            = 0
    values_msg_id             = 0
    aix_values                = ""
    help_msg_id               = "04201"
    help_msg_loc              = ""

