#------------------------ nim_spot_cust            ----------------------------

sm_cmd_hdr:
		id = "nim_spot_cust"
		option_id = "nim_auto_expand,nim_installp_flags,nim_spot_com,nim_spot_cust_opt,nim_preview"
		has_name_select = "y"
		name = "Customize a SPOT"
		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 91
		cmd_to_exec = "cust() 
{
	FORCE= 
	BUNDLE= 
	LPP_SOURCE= 
	OPTIONS= 
	FIXBUNDLE=
	FIXES=
	FLAGS= 
	NAME=$1 
	shift 
	while [ $# -gt 0 ] 
	do 
		case $1 in 
			-b)	BUNDLE=$2; shift 2;; 
			-l)	LPP_SOURCE=$2; shift 2 
					lsnim ${LPP_SOURCE} >/dev/null 2>&1 || LPP_SOURCE="/dev/${LPP_SOURCE}";; 
			-a)	AUTOE=TRUE; shift;;
			-o)	OPTIONS=$2; shift 2;; 
			-F)	FORCE=-F; shift 1;; 
			-x)		shift;; 
			-f)	FLAGS=${FLAGS}${2}; shift 2;;
			-B) 	FIXBUNDLE=$2; shift 2;;
			-k) 	FIXES=$2; shift 2;;
			*) shift;; 
		esac 
	done 
        [[ -n ${FLAGS} ]] &&  FLAGS=a${FLAGS}
        [[ ${FLAGS} = *N* ]] && [[ ${FLAGS} != *c* ]] && FLAGS=c${FLAGS}
	nim -o cust $FORCE ${BUNDLE:+-a installp_bundle=$BUNDLE} \
		${LPP_SOURCE:+-a lpp_source=$LPP_SOURCE} \
		${FLAGS:+-a installp_flags=-$FLAGS} \
		${FIXBUNDLE:+-a fix_bundle=$FIXBUNDLE} \
		${FIXES:+-a fixes=\"$FIXES\"} \
		${OPTIONS:+-a filesets=\"$OPTIONS\"} ${AUTOE:+-a auto_expand=no} $NAME
	exit $?
}
cust"
		ask = "y"
		exec_mode = ""
		ghost = "n"
		cmd_to_discover = "echo \"#auto_expand\n-x\""
		cmd_to_discover_postfix = ""
		name_size = 0
		value_size = 0
		help_msg_id = "0042347"
