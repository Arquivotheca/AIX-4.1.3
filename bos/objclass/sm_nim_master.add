#------------------------ nim_chmac_hdr_                       ----------------

sm_cmd_hdr:
		id = "nim_chmac_hdr_"
		option_id = "nim_chmac_opt"
		has_name_select = "y"
		name = "Change/Show Characteristics of a Machine Object"
		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 16
		cmd_to_exec = "change_machine() 
{
	change= 
	while [[ $# -gt 0 ]]
	do
		case $1 in 
			-N) NAME=$2;;
			-n) NETNAME=$2;net=TRUE;change=TRUE;;
			-h) HOSTNAME=$2;hostname=TRUE;change=TRUE;;
			-a) HADDR=$2;haddr=TRUE;change=TRUE;;
			-l) ADAPTER=$2;adpt=TRUE;change=TRUE;;
			-r) RSPEED=$2;change=TRUE;;
			-T) CABLE=$2;change=TRUE;;
			-C) CPUID=$2;change=TRUE;;
			-c) COMMENTS=$2;C_change=TRUE;change=TRUE;;
			-E) IPLROM_EMU=\"-a iplrom_emu=$2\";change=TRUE;;
		esac 
		shift 2
	done

	pif=`lsnim -a if1 $NAME | awk \'NR==2{$1=\"\";$2=\"\";print}\'` 
	pif_net=`echo $pif | awk \'{print $1}\'` 
	pif_hostname=`echo $pif | awk \'{print $2}\'` 
	pif_haddr=`echo $pif | awk \'{print $3}\'` 
	pif_adpt=`echo $pif | awk \'{print $4}\'` 

	if [ -n \"$net\" ] || [ -n \"$hostname\" ] || [ -n \"$haddr\" ] || \
		[ -n \"$adpt\" ] 
	then
		[ -n \"$net\" ] && PIF=\"$NETNAME\" || PIF=\"$pif_net\"
		[ -n \"$hostname\" ] && PIF=\"$PIF $HOSTNAME\" || \
		PIF=\"$PIF $pif_hostname\"
		[ -n \"$haddr\" ] && PIF=\"$PIF $HADDR\" || PIF=\"$PIF $pif_haddr\"
		[ -z \"$adpt\" ] && ADAPTER=$pif_adpt
		[ -n \"$ADAPTER\" ] && PIF=\"$PIF $ADAPTER\" 
	fi

	if [[ -n \"$change\" ]]
	then 
	nim -o change ${PIF:+-a if1=\"$PIF\"} \
			${RSPEED:+-a ring_speed1=$RSPEED} ${CABLE:+-a cable_type1=$CABLE} \
			${IPLROM_EMU} \
			${CPUID:+-a cpuid=$CPUID} \
			${C_change:+-a comments=\"$COMMENTS\"} $NAME 
		exit $?
	else 
		exit 0 
	fi
}
