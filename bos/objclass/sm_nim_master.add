#------------------------ nim_mkmac_if_tok                           ----------

sm_cmd_hdr:
		id = "nim_mkmac_if_tok"
		option_id = "nim_mkmac_if_com,nim_mkmac_if_tok_opt"
		has_name_select = "y"
		name = "Define a Network Install Interface"
		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 22
		cmd_to_exec = "mkmac_if()
{
	NAME=$1
	HADDR=0
	SEQNO=$( lsnim -A if $NAME )
	shift
	while getopts n:h:a:l:r:T: FLAG
	do
		case $FLAG in
			n) NETNAME=$OPTARG;;
			h) HOSTNAME=$OPTARG;;
			a) HADDR=$OPTARG;;
			l) ADAPTER=$OPTARG;;
			r) RSPEED=\"ring_speed$SEQNO=$OPTARG\";;
		esac
	done
	[ -n \"$ADAPTER\" ] && 
		IF=\"$NETNAME $HOSTNAME $HADDR $ADAPTER\" ||
		IF=\"$NETNAME $HOSTNAME $HADDR\"
	nim -o change ${IF:+-a if$SEQNO=\"$IF\"} ${RSPEED:+-a $RSPEED} $NAME 
	exit $?
}
