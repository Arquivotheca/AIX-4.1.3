		name = "Define a Machine Object"
		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 15
		cmd_to_exec = "add_machine()
{
	HADDR=0
	while getopts N:t:P:T:n:h:a:l:r:C:c:E: FLAG
	do
		case $FLAG in
			N) NAME=$OPTARG;;
			t) TYPE=$OPTARG;;
			P) PLATFORM=$OPTARG;;
			T) CABLE=$OPTARG;;
			n) NETNAME=$OPTARG;;
			h) HOSTNAME=$OPTARG;;
			a) HADDR=$OPTARG;;
			l) ADAPTER=$OPTARG;;
			r) RSPEED=$OPTARG;;
			C) CPUID=$OPTARG;;
			c) COMMENTS=$OPTARG;;
			E) IPLROM_EMU=$OPTARG;;
		esac
	done
	[ -n \"$ADAPTER\" ] && \
		PIF=\"$NETNAME $HOSTNAME $HADDR $ADAPTER\" || \
		PIF=\"$NETNAME $HOSTNAME $HADDR\"
	nim -o define -t $TYPE -a platform=$PLATFORM -a if1=\"$PIF\" \
		${RSPEED:+-a ring_speed1=$RSPEED} ${CABLE:+-a cable_type1=$CABLE} \
		${IPLROM_EMU:+-a iplrom_emu=$IPLROM_EMU} \
		${CPUID:+-a cpuid=$CPUID} ${COMMENTS:+-a comments=\"$COMMENTS\"} $NAME
	exit $?
}
