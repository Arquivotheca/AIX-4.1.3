#------------------------ nim_mkres_spot           ----------------------------

sm_cmd_hdr:
		id = "nim_mkres_spot"
		option_id = "nim_spot_opt,nim_mkres_opt,nim_auto_expand,nim_installp_flags"
		has_name_select = "y"
		name = "Define a Resource Object"
		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 29
		cmd_to_exec = "mkres() 
{
	while getopts aN:t:s:S:l:c:f:x option
	do 
		case $option in 
			a) AUTOE=TRUE;;
			N) NAME=$OPTARG;;
			t) TYPE=$OPTARG;;
			s) SERVER=$OPTARG;;
			S) SOURCE=$OPTARG
				if lsnim ${SOURCE} >/dev/null 2>&1
				then
					:
				elif [[ -n \"$( /usr/sbin/lsdev -Cl ${SOURCE} 2>&1 )\" ]]
				then
					SOURCE="/dev/${SOURCE}"
				fi
				;;
			l) LOCATION=$OPTARG;;
			c) COMMENTS=$OPTARG;;
			f) FLAGS=${FLAGS}$OPTARG;;
		esac 
	done 
        [[ -n ${FLAGS} ]] &&  FLAGS=aQ${FLAGS}
        [[ ${FLAGS} = *N* ]] && [[ ${FLAGS} != *c* ]] && FLAGS=c${FLAGS}
	nim -o define -t $TYPE -a server=$SERVER -a location=$LOCATION \
		-a source=$SOURCE ${COMMENTS:+-a comments=\"$COMMENTS\"} \
		${AUTOE:+-a auto_expand=no} ${FLAGS:+-a installp_flags=-${FLAGS}} \
		$NAME
	exit $?
}
mkres"
		ask = ""
		exec_mode = ""
		ghost = "n"
		cmd_to_discover = "echo \"#auto_expand\n-x\n\""
