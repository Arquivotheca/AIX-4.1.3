		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 11
		cmd_to_exec = "add_route()
{
	while getopts o:O:d:D:F: option
	do
		case $option in
			o) ORIGIN=$OPTARG;;
			O) OGATEWAY=$OPTARG;;
			d) DEST=$OPTARG;;
			D) DGATEWAY=$OPTARG;;
			F) FORCE=yes;;
		esac
	done

	SEQNO=$( lsnim -A routing $ORIGIN )
	nim -o change ${FORCE:+-a force=yes} \
		-a routing$SEQNO=\"$DEST $OGATEWAY $DGATEWAY\" $ORIGIN
	exit $?
}
