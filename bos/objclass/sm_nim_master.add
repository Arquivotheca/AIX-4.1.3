	NO_NIM_CLIENT= 
	NO_BOOT=
	FORCE_PUSH=
	SET_BOOTLIST=
	FLAGS= 
	OPTS= 
	while getopts N:s:f:o:nbpl option
	do
		case $option in
			N) NAME=$OPTARG;;
			s) SOURCE=$OPTARG;;
			f) FLAGS=$OPTARG;;
			o) OPTS=$OPTARG;;
			n) NO_NIM_CLIENT=\"-a no_nim_client=yes\";;
			b) NO_BOOT=\"-a no_client_boot=yes\";;
			p) FORCE_PUSH=\"-a force_push=yes\";;
			l) SET_BOOTLIST=\"-a set_bootlist=yes\";;
		esac 
	done 
	nim -o bos_inst ${SOURCE:+-a source=$SOURCE} \
		${FLAGS:+-a installp_flags=$FLAGS} \
		${OPTS:+-a filesets=\"$OPTS\"} \
		$NO_NIM_CLIENT $NO_BOOT $FORCE_PUSH $SET_BOOTLIST $NAME 
	exit $?
}
