		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 21
		cmd_to_exec = "lsmac_if()
{
	if [ $# -gt 0 ] 
	then 
		while [ $# -gt 0 ] 
		do 
