#------------------------ nim_mkres_lpp_source     ----------------------------

sm_cmd_hdr:
		id = "nim_mkres_lpp_source"
		option_id = "nim_mkres_opt,nim_lpp_source_opt"
		has_name_select = "y"
		name = "Define a Resource Object"
		name_msg_file = "cmdnim.cat"
		name_msg_set = 10
		name_msg_id = 29
		cmd_to_exec = "mkres() 
{
	while getopts N:t:s:S:l:c:O: option
	do 
		case $option in 
			N) NAME=$OPTARG;;
			t) TYPE=$OPTARG;;
			s) SERVER=$OPTARG;;
			S) SOURCE=$OPTARG
				if lsnim ${SOURCE} >/dev/null 2>&1
				then
					:
				elif [[ -n \"$( /usr/sbin/lsdev -Cl ${SOURCE} 2>&1 )\" ]]
				then
					SOURCE="/dev/${SOURCE}"
				fi
				;;
			l) LOCATION=$OPTARG;;
			c) COMMENTS=$OPTARG;;
			O) OPTIONS=$OPTARG;;
		esac 
	done 

	nim -o define -t $TYPE -a server=$SERVER -a location=$LOCATION \
		${SOURCE:+-a source=${SOURCE}} ${COMMENTS:+-a comments=\"$COMMENTS\"} \
		${OPTIONS:+-a packages=\"${OPTIONS}\"} $NAME
	exit $?
}
