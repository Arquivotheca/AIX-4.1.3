sm_name_hdr:
	id                              = "chlv1"
	next_id                         = "chlvd"
	option_id                       = "_cmdlvmlvns"
	name                            = "Change a Logical Volume"
	name_msg_file                   = "smit.cat"
	name_msg_set                    = 15
	name_msg_id                     = 257
	type                            = "j"
	ghost                           = ""
	cmd_to_classify                 = "lvcls () { LVID=`getlvodm -l $1`; \
if test $? -ne 0\nthen\n  exit 1\nfi\n\
Q1=`getlvodm -c $LVID | cut -f2-6,8 -d' ' | sed '1s/ /:/g'`; \
Q4=`getlvodm -B $LVID`; \
Q2=`lquerylv -L $LVID -nMPRv`; \
Q3=`echo $Q2 |sed '1s/ /:/g'`; \
Q5=`lquerylv -L $LVID -w`; \
echo $1:$Q1:$Q4:$Q3:$Q5; }; \
lvcls"
	cmd_to_classify_postfix                 = "_rawname"
	raw_field_name                 = "_rawname"
	cooked_field_name                 = "LVName:LVType:Position:\
Range:Upbound:Strict:Reloc:Label:MaxLPs:Sched:Permissions:BadBlk:Verify:MirrWC"
	next_type                 = "d"
	help_msg_id                     = "0516000"
	help_msg_loc                     = ""

sm_cmd_hdr:
	id                              = "chlvd"
	option_id                       = "_cmdlvmchlv"
	has_name_select                 = "y"
	name                            = "Change a Logical Volume"
	name_msg_file                   = "smit.cat"
	name_msg_set                    = 15
	name_msg_id                     = 257
	cmd_to_exec                     = "chlv"
	ask                             = "n"
	exec_mode                       = ""
	ghost                           = ""
	cmd_to_discover                 = "lvdisc () { Sched='p'; Perm='r'; BadBlk='n'; Verify='n'; MirrWC='n'; \
if test "$1" -eq 1\nthen\n  Sched='s'\nfi\n\
if test "$2" -eq 1\nthen\n  Perm='w'\nfi\n\
if test "$3" -eq 1\nthen\n  BadBlk='y'\nfi\n\
if test "$4" -eq 1\nthen\n  Verify='y'\nfi\n\
if test "$5" -eq 1\nthen\n  MirrWC='y'\nfi\n\
echo '#:::::::::Sched:Permissions:BadBlk:Verify:MirrWC'; \
echo ':::::::::'$Sched':'$Perm':'$BadBlk':'$Verify':'$MirrWC; }; \
lvdisc"
	cmd_to_discover_postfix         = "Sched Permissions BadBlk Verify MirrWC"
	name_size                       = 0
	value_size                      = 0
	help_msg_id                     = "0516000"
	help_msg_loc                     = ""

