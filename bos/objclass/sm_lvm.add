sm_name_hdr:
	id                              = "chpv"
	next_id                         = "chpv1"
	option_id                       = "_cmdlvmpvns"
	name                            = "Change Characteristics of a Physical Volume"
	name_msg_file                   = "smit.cat"
	name_msg_set                    = 15
	name_msg_id                     = 20
	type                            = "j"
	ghost                           = ""
	cmd_to_classify                 = "pvcls () { PVID=`getlvodm -p $1`; \
if test $? -ne 0\nthen\n  exit 1\nfi\n\
Alloc='y'; Avail='r'; \
VGID=`getlvodm -j $1`; \
if test $? -ne 0\nthen\n  exit 1\nfi\n\
Q1=`lquerypv -p $PVID -g $VGID -c`; \
Q2=`expr $Q1 % 2`;Q3=`expr $Q1 / 8`;Q3=`expr $Q3 % 2`; \
if test "$Q2" -eq 1\nthen\n  Avail='a'\nfi\n\
if test "$Q3" -eq 1\nthen\n  Alloc='n'\nfi\n\
echo "$Alloc"":""$Avail"; }; \
pvcls"
	cmd_to_classify_postfix                 = "_rawname"
	raw_field_name                 = "_rawname"
	cooked_field_name                 = "Alloc:Avail"
	next_type                 = "d"
	help_msg_id                     = "0516020"
