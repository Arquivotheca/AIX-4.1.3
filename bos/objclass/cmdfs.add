	name_msg_set        = 19
	name_msg_id         = 50
	op_type             = "l"
	entry_type          = "t"
	entry_size          = 0
	required            = ""
	prefix              = "--"
	cmd_to_list_mode    = "1"
	cmd_to_list         = "lsfs | while read dev node mnt vfs size opts jnk
do
        if mount | grep -q $dev
        then 
                echo $dev
        else
                echo $dev $mnt $vfs $size
        fi
done | awk 'NR > 1 { printf \"%-20s\t%-20s\t%-5s\t%-10s\\n\", $1, $2, $3, $4 }'"
	cmd_to_list_postfix = ""
	multi_select        = ""
