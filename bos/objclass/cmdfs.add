	entry_type          = "t"
	entry_size          = 0
	required            = ""
	prefix              = "--"
	cmd_to_list_mode    = "1"
	cmd_to_list         = "for mntpt in `lsfs | awk 'NR > 1 { print $3 }'` ; do mount | sed -e 1d -e 2d -e 's/^[^ 	]*//' | awk '{ print $2 }' | grep -s \^$mntpt\$ || lsfs $mntpt | awk 'NR > 1 { printf \"%-20s\t%-20s\t%-5s\t%-10s\\n\", $3, $1, $4, $5 }' ; done"
	cmd_to_list_postfix = ""
